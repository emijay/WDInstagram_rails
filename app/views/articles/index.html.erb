


<%= link_to 'New article', new_article_path %>

<table>
  <tr>
    <th>Author's Name</th>
    <th>Photo Url</th>
    <th>Title</th>
    <th></th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.author_name %></td>
      <td><%= article.photo_url %></td>
      <td><%= article.title%></td>
      <td><%= link_to 'Show', article_path(article) %></td>
    </tr>
  <% end %>
</table>